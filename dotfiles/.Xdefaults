Xft.dpi:        96
Xft.antialias:  true
Xft.hinting:    true
Xft.rgba:       rgb
Xft.hintstyle:  hintslight

Xcursor.size: 8
Xcursor.theme: Alkano-Aluminium

!URxvt
URxvt.depth: 24
URxvt.letterSpace: -1
URxvt.scrollBar: false
URxvt.geometry: 120x30
URxvt.internalBorder: 15
URxvt.font: Monaco for Powerline:size=9:antialias=true, Font Awesome 5 Free:style=Solid:size=9
URxvt.boldFont:

URxvt*termName:rxvt-unicode-256color

